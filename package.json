    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@types/node": "^20.6.3",
    "date-fns": "^2.30.0",
    "dotenv": "^16.3.1",
    "fastify": "^4.23.2",
    "knex": "^2.5.1",
    "pg": "^8.11.3",
    "winston": "^3.10.0"
  }
}
